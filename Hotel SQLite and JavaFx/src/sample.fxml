<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#menuFileCloseClicked" text="Close" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="About" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <HBox spacing="10.0">
               <children>
                  <TextField promptText="username" />
                  <TextField layoutX="10.0" layoutY="10.0" promptText="password" />
                  <Region HBox.hgrow="ALWAYS" />
                  <Button mnemonicParsing="false" onAction="#loginButtonClicked" text="Login In" />
                  <Button layoutX="384.0" layoutY="10.0" mnemonicParsing="false" onAction="#settingsButtonClicked" text="Settings" />
               </children>
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </top>
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="lblinput" text="Something" />
         </children>
         <BorderPane.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
   <center>
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab text="Employee">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button layoutX="63.0" layoutY="188.0" mnemonicParsing="false" onAction="#buttonShowEmployees" prefHeight="31.0" prefWidth="157.0" text="Show Employees" />
                        <Button layoutX="78.0" layoutY="133.0" mnemonicParsing="false" onAction="#buttonAddEmployee" prefHeight="31.0" prefWidth="127.0" text="Add Employee" />
                        <TextField layoutX="53.0" layoutY="38.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Rooms">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab fx:id="Guest" text="Guest">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="tfname" layoutX="51.0" layoutY="29.0" text="name" />
                        <TextField fx:id="tfsurname" layoutX="51.0" layoutY="68.0" text="surname" />
                        <TextField fx:id="tfpesel" layoutX="51.0" layoutY="107.0" text="pesel" />
                        <Button layoutX="86.0" layoutY="148.0" mnemonicParsing="false" onAction="#buttonAddGuest" text="New Guest" />
                        <Button layoutX="70.0" layoutY="207.0" mnemonicParsing="false" onAction="#buttonShowGuest" text="Show guest list" />
                     </children></AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </center>
   <right>
      <VBox prefHeight="296.0" prefWidth="311.0" BorderPane.alignment="CENTER">
         <children>
             <TextField fx:id="input" layoutX="10.0" layoutY="45.0" prefHeight="31.0" prefWidth="272.0">
             </TextField>
            <Button mnemonicParsing="false" onAction="#buttonSomething" prefHeight="31.0" prefWidth="170.0" text="Click me" />
         </children>
      </VBox>
   </right>
</BorderPane>
